{
  "name": "jest-code-snippets",
  "displayName": "jest-code-snippets",
  "description": "Code snippets for testing framework Jest",
  "publisher": "wanglihua",
  "icon": "images/jest_snippets_icon.png",
  "version": "0.0.4",
  "license": "MIT",
  "scripts": {
    "format:prettier": "prettier --write *.md snippets/*",
    "format": "npm run format:prettier",
    "publish": "vsce publish",
    "package": "vsce package"
  },
  "engines": {
    "vscode": "^1.58.0"
  },
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/snippets.code-snippets"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets/snippets.code-snippets"
      },
      {
        "language": "typescript",
        "path": "./snippets/snippets.code-snippets"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/snippets.code-snippets"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/qluwlh/jest-code-snippets.git"
  },
  "homepage": "https://github.com/qluwlh/jest-code-snippets/blob/main/README.md",
  "devDependencies": {
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "vsce": "^2.6.7"
  }
}
